{
	"multipart": [
		{   "apply": { "model": "improbableplotmachine:block/blast_furnace_pipe/center" }},
		{   "when": { "north": true },
			"apply": { "model": "improbableplotmachine:block/blast_furnace_pipe/part", "x": -90 }
		},
		{   "when": { "east": true },
			"apply": { "model": "improbableplotmachine:block/blast_furnace_pipe/part", "x": 90, "y": -90 }
		},
		{   "when": { "south": true },
			"apply": { "model": "improbableplotmachine:block/blast_furnace_pipe/part", "x": 90 }
		},
		{   "when": { "west": true },
			"apply": { "model": "improbableplotmachine:block/blast_furnace_pipe/part", "x": 90, "y": 90 }
		},
		{   "when": { "up": true },
			"apply": { "model": "improbableplotmachine:block/blast_furnace_pipe/part", "x": 180 }
		},
		{   "when": { "down": true },
			"apply": { "model": "improbableplotmachine:block/blast_furnace_pipe/part" }
		}
	]
}